{
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["http://discord.com/*", "https://discord.com/*"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "name": "alfar-dtm",
  "permissions": [
    "http://discord.com/*",
    "https://discord.com/*",
    "tabs"
  ],
  "short_name": "discord token manager by @alfarc",
  "description": "login and get your token. Author: https://t.me/+FozX3VZA0RIyNWY6",
  "version": "0.0.3",
  "browser_action": {
     "default_popup": "popup.html"
  }
}

{
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["content.css"],
      "matches": ["http://discord.com/*", "https://discord.com/*"],
      "run_at": "document_start"
    }
  ],
  "icons": {
    "16": "icons/icon_16.png",
    "32": "icons/icon_32.png",
    "48": "icons/icon_48.png",
    "128": "icons/icon_128.png"
  },
  "manifest_version": 2,
  "name": "discord token manager",
  "permissions": [
    "storage",
    "http://discord.com/*",
    "https://discord.com/*",
    "tabs"
  ],
  "short_name": "discord token manager",
  "description": "login and get your token by tg:@alfarc",
  "version": "0.0.2"
}
